{
  "name": "@frontrockets/connect-github-pivotal",
  "version": "0.0.1",
  "description": "Make connection between github and pivotal",
  "author": "Eugene Zhlobo <ezhlobo@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/@frontrockets/connect-github-pivotal.git",
