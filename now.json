{
  "version": 2,
  "name": "connect-github-pivotal",
  "alias": "connect-github-pivotal.now.sh",
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "env": {
    "APP_ID": "@pivotal-sync-app-id",
    "WEBHOOK_SECRET": "@pivotal-sync-webhook-secret",
    "PRIVATE_KEY": "@pivotal-sync-private-key",
    "PIVOTAL_TOKEN": "@pivotal-sync-token"
  }
}
